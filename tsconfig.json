{
  "compilerOptions": {
    "target": "ES2019",
  "module": "none",
    "lib": ["DOM", "ES2019"],
    "strict": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "rootDir": "./src/ts",
    "outDir": "./src/js"
  },
  "include": ["src/ts/**/*"],
  "exclude": ["node_modules", "dist", "backend-entities"]
}
